  function initMap() {
   // var map = L.map('map').setView([40.67817274214118, -3.706116278658542], 2); // Coordenadas del centro del mapa
   var map = L.map('map').setView([40.665, -3.7], 20); // Ajusta las coordenadas y el nivel de zoom

    L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
      attribution: '&copy; <a href="https://www.openstreetmap.org/copyright">OpenStreetMap</a> contributors'
    }).addTo(map);

    // Define las coordenadas de inicio y fin de la ruta
    var startPoint = L.latLng(40.658009008271584, -3.7664719171884222);
    var endPoint = L.latLng(40.6839090453442, -3.650114432529451);

