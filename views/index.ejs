    var endPoint1 = L.latLng(40.658009008271584, -3.7664719171884222); 

    // Crea un objeto de control de rutas y especifica el modo de transporte (andando o en coche)
    var routeControl = L.Routing.control({
      waypoints: [startPoint, endPoint],
      routeWhileDragging: true, // Permitir que se actualice la ruta mientras se arrastra el marcador final
      router: new L.Routing.OSRMv1({
        serviceUrl: 'https://router.project-osrm.org/route/v1'
      }), // Servicio OSRM para rutas
      show: false // Ocultar las instrucciones de cómo ir
    }).addTo(map);


